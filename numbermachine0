import math
class numbermachine0:
  #requires import math
  def __init__(self,raw):
    self.data = sorted(raw)
    self.size = len(raw)
  LowQTarget = 25
  UppQTarget = 75
  def PtLow(self):
    return self.data[0]
  def PtTop(self):
    return self.data[-1]
  def PtMed(self):
    return (list(reversed(self.data))[self.size//2] + self.data[self.size//2]) / 2
  def PtQLow(self):
    return (list(reversed(self.data))[int(self.size-(self.size/100*self.LowQtTarget))] + self.data[int(self.size/100*self.LowQtTarget)]) / 2
  def PtQTop(self):
    return (list(reversed(self.data))[int(self.size-(self.size/100*self.UppQtTarget))] + self.data[int(self.size/100*self.UppQtTarget)]) / 2
